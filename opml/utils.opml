				<outline text="if (!isWhite (s.charAt (0))) {">
					<outline text="break;"></outline>
					<outline text="}"></outline>
					</outline>
				<outline text="s = s.substr (1);"></outline>
				<outline text="}"></outline>
				</outline>
			<outline text="while (s.length &gt; 0) {">
